{
  "name": "blog-site",
  "private": true,
  "version": "0.0.2",
  "scripts": {
    "dev": "vite --host --port=8000",
    "server:mock": "nodemon -w mock --ext .ts --exec ts-node -T ./server/index.ts",
    "dev:mock": "npm run dev & npm run server:mock",
    "build": "vite build",
    "preview": "vite preview",
    "prettier": "prettier --write **/*.{js,tsx,ts,less,md,json}",
    "lint:ci": "npm run eslint:ci",
    "eslint:ci": "eslint --cache --ext .tsx,.ts,.js ./src",
    "eslint:fix": "eslint --cache --ext .tsx,.ts,.js ./src --fix",
    "stylelint:ci": "stylelint ./src/**/*.less --custom-syntax postcss-scss",
    "stylelint:fix": "stylelint --fix ./src/**/*.less --custom-syntax postcss-scss",
    "lint:fix": "npm run stylelint:fix && npm run eslint:fix",
    "pre-commit": "lint-staged",
    "publish": "npm run build && sudo scp -r ./dist/* root@wishufree.com:/root/wish_server/apps/blog/public",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/joy": "^5.0.0-beta.36",
    "@mui/material": "^5.15.15",
    "@types/lodash": "^4.14.186",
    "@wangeditor/editor": "^5.1.22",
    "@wangeditor/editor-for-react": "^1.0.6",
    "ahooks": "^3.5.0",
    "animate.css": "^4.1.1",
    "antd": "^5.16.1",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.10",
    "eslint": "^8.17.0",
    "history": "^5.3.0",
    "js-cookie": "^3.0.1",
    "js-file-downloader": "^1.1.24",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lodash": "^4.17.21",
    "markdown-navbar": "^1.4.3",
    "particles-bg": "^2.5.5",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^2.3.1",
    "qs": "^6.11.0",
    "query-string": "^9.0.0",
    "ramda": "^0.28.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-markdown": "^9.0.1",
    "react-markdown-editor-lite": "^1.3.3",
    "react-masonry-css": "^1.0.16",
    "react-photo-view": "^1.2.6",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "rehype-video": "^2.0.2",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remark-ins": "^1.1.0",
    "umi-request": "^1.4.0",
    "yet-another-react-lightbox": "^3.19.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@rollup/plugin-html": "^1.0.3",
    "@types/crypto-js": "^4.1.1",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-navbar": "^1.4.0",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^17.0.45",
    "@types/path-browserify": "^1.0.2",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.14",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-infinite-scroller": "^1.2.5",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.0",
    "colors-console": "^1.0.3",
    "cors": "^2.8.5",
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "express": "^4.18.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mockjs": "^1.1.0",
    "nodemon": "^2.0.16",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "prettier-plugin-classify-imports": "^0.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:ci"
    ],
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  }
}
